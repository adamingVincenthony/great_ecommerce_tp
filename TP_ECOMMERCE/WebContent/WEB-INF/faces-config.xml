<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>succes</from-outcome>
			<to-view-id>/accueilAdmin.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>echec</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/searchCategorie.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToCategorie</from-outcome>
			<to-view-id>/accueilShowProduit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/searchProduitKW.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToProduit</from-outcome>
			<to-view-id>/pageProduit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/accueilShowProduit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToProduit</from-outcome>
			<to-view-id>/pageProduit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/panier.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToPanier</from-outcome>
			<to-view-id>/panierAlt.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

<navigation-rule>
		<from-view-id>/panierAlt.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToPanier</from-outcome>
			<to-view-id>/panier.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
